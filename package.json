{
  "name": "lambda-bench",
  "version": "1.0.0",
  "description": "Lambda benchmark CLI",
  "main": "dist/index.js",
  "bin": {
    "lb": "./dist/index.js"
  },
  "author": "vieirai",
  "license": "ISC",
  "repository": "https://github.com/vieirai/lambda-bench",
  "homepage": "https://github.com/vieirai/lambda-bench#readme",
  "scripts": {
    "build": "esbuild --bundle src/index.ts --outdir=dist --minify --sourcemap --platform=node",
    "lint": "eslint 'src/**/*.ts' --fix && tsc"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.413.0",
    "axios": "^1.6.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^11.0.0",
    "ora": "^7.0.1",
    "stats-lite": "^2.2.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^20.6.1",
    "@types/stats-lite": "^2.2.1",
    "@typescript-eslint/parser": "^6.13.2",
    "bun-types": "^1.0.15",
    "esbuild": "^0.19.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "import-sort-style-module": "^6.0.0",
    "prettier": "^3.0.2",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "importOrderSeparation": true,
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "^[./]"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "lambda",
    "benchmark",
    "cold",
    "start",
    "cold-start",
    "cli",
    "aws"
  ]
}
